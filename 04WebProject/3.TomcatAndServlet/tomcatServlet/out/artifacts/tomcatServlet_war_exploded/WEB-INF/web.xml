<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <!--        配置信息-->
    <servlet>
        <servlet-name>Demo01_servlet01</servlet-name>
        <servlet-class>cn.kk.Demo01_servlet01</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Demo01_servlet01</servlet-name>
        <url-pattern>/demo01</url-pattern>
    </servlet-mapping>

    <!--    <load-on-startup></load-on-startup>
            负整数：第一次访问被创建（第一次访问时，init输出）
          0或正整数：服务器启动时创建（即启动时，init输出）
        -->
    <servlet>
        <servlet-name>Demo02_servlet02</servlet-name>
        <servlet-class>cn.kk.Demo02_servlet02</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>Demo02_servlet02</servlet-name>
        <url-pattern>/demo02</url-pattern>
    </servlet-mapping>

</web-app>